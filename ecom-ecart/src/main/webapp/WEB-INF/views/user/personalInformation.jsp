<div class="col-md-9 col-sm-9 col-xs-12">
	<div class="panel panel-primary">
		<div class="panel-heading">
			<div class="panel-title">Personal Information</div>
		</div>

		<div
			class="panel-body col-md-offset-3 col-sm-offset-3 col-xs-offset-3">
			<form>
				<div class="col-md-9 col-sm-9 col-xs-9">
					<div class="table-responsive">
						<table class="table" id="no-border">
							<tbody>
								<tr>
									<td><label for="firstName">First
											Name</label></td>
									<td><div class="form-group">
											<input type="text" class="form-control"
												placeholder="First Name" name="firstName" id = "firstName">
										</div></td>
								</tr>
								<tr>
									<td><label for="lastName">Last Name</label></td>
									<td><div class="form-group">
											<input type="text" class="form-control"
												placeholder="Last Name" name="lastName" id = "lastName">
										</div></td>
								</tr>
								<tr>
									<td colspan="2">
										<div class="form-group">
											<input type="submit"
												class="btn btn-primary col-md-4 col-sm-6 col-xs-6 col-md-offset-4 col-sm-offset-4 col-xs-offset-4"
												value="Save">
										</div>
									</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>